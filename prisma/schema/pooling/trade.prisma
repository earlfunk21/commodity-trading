model Trade {
  id            String             @id @default(nanoid())
  createdAt     DateTime           @default(now())
  updatedAt     DateTime           @updatedAt
  deletedAt     DateTime?
  mainTokenId   String
  mainToken     MainToken          @relation(fields: [mainTokenId], references: [id])
  capital       Float
  unitValue     Float
  soldValue     Float
  quantity      Float
  grossSales    Float
  managementFee Float
  grossIncome   Float
  tax           Float
  netIncome     Float
  tokenValue    Float
  transactions  TradeTransaction[]
}
