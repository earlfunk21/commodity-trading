model SubTokenTransaction {
  id                 String           @id @default(nanoid(16))
  createdAt          DateTime         @default(now())
  updatedAt          DateTime         @updatedAt
  deletedAt          DateTime?
  subTokenId         String
  subToken           SubToken         @relation(fields: [subTokenId], references: [id])
  tokenValue         Float
  tradeTransactionId String
  tradeTransaction   TradeTransaction @relation(fields: [tradeTransactionId], references: [id])
}
