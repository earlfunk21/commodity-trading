model MainTokenValue {
  id                String          @id @default(nanoid())
  createdAt         DateTime        @default(now())
  updatedAt         DateTime        @updatedAt
  deletedAt         DateTime?
  commodityId       String
  commodity         Commodity       @relation(fields: [commodityId], references: [id])
  commodityTypeId   String
  commodityType     CommodityType   @relation(fields: [commodityTypeId], references: [id])
  mainToken         MainToken       @relation("TokenValues", fields: [mainTokenId], references: [id])
  mainTokenId       String
  totalValue        Float
  unitValue         Float
  volume            Float
  soldTokens        Float           @default(0)
  purchaseTokens    PurchaseToken[]
  subTokens         SubToken[]
  currentMainTokens MainToken[]     @relation("CurrentTokenValue")
  lastMainTokens    MainToken[]     @relation("LastTokenValue")
}
