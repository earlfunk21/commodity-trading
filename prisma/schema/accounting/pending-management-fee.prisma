model PendingManagementFee {
  id                            String               @id @default(nanoid())
  createdAt                     DateTime             @default(now())
  updatedAt                     DateTime             @updatedAt
  deletedAt                     DateTime?
  amount                        Float                @default(0)
  itManagementAmount            Float                @default(0)
  partnersManagementAmount      Float                @default(0)
  tpcpiReferrerManagementAmount Float                @default(0)
  tpcpiManagementAmount         Float                @default(0)
  mainTokenId                   String
  mainToken                     MainToken            @relation(fields: [mainTokenId], references: [id])
  mainTokenTransactionId        String               @unique
  mainTokenTransaction          MainTokenTransaction @relation(fields: [mainTokenTransactionId], references: [id])
  releasedAt                    DateTime?

  @@index([releasedAt])
}
