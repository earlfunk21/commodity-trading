model TPCPIReferrerCommission {
  id                     String               @id @default(nanoid())
  createdAt              DateTime             @default(now())
  updatedAt              DateTime             @updatedAt
  deletedAt              DateTime?
  userId                 String
  user                   User                 @relation(fields: [userId], references: [id])
  amount                 Float
  mainTokenId            String               @unique
  mainToken              MainToken            @relation(fields: [mainTokenId], references: [id])
  mainTokenTransactionId String               @unique
  mainTokenTransaction   MainTokenTransaction @relation(fields: [mainTokenTransactionId], references: [id])
  releasedAt             DateTime?

  @@index([releasedAt])
}
