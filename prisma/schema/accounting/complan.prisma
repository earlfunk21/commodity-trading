model Complan {
  id                            String                         @id @default(nanoid(16))
  createdAt                     DateTime                       @default(now())
  updatedAt                     DateTime                       @updatedAt
  deletedAt                     DateTime?
  name                          String
  commission                    Float                          @default(0)
  tax                           Float                          @default(0)
  referralCommission            Float                          @default(0)
  pendingReferralCommission     Float                          @default(0)
  managementFee                 Float                          @default(0)
  pendingManagementFee          Float                          @default(0)
  itManagement                  Float                          @default(0)
  partnersManagement            Float                          @default(0)
  tpcpiReferrerManagement       Float                          @default(0)
  tpcpiManagement               Float                          @default(0)
  capital                       Float                          @default(0)
  mainTokens                    MainToken[]
  mainTokenTransactions         MainTokenTransaction[]
  tradeTransactions             TradeTransaction[]
}
